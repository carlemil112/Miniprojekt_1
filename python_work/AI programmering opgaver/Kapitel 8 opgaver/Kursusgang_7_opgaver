import pygame
import sys

def create_grid():
    grid = [[0 for i in range(48)]for i in range(64)] #64x48 grid fyldt med 0'er
    return grid #Returnerer grid når man kører funktionen


def main_function():
    grid = create_grid()

    for row in grid:
        print(row)
    
main_function()

